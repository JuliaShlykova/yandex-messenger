<div class="{{#if class}}{{class}} {{/if}}chat-item-wrapper">
  <div class="avatar">
    <img src="/avatar.svg" alt="">
  </div>
  <div class="chat-info">
    <div class="chat-info-header">
      <span class="chat-name">
        {{title}}
      </span>
      <div class="chat-time">
        {{transformToDay last_message.time}}
      </div>
    </div>
    <div class="last-msg">
      {{last_message.content}}
    </div>

  </div>
</div>
