<div class="pop-up-back">
<div class="centering-wrapper">
  <div class="block">
  <div>
    <h1>Текущие участники</h1>
    <ul>
      {{#each participants}}
      <li>
      <div class="participant-avatar">
        <img src="{{setAvatar this}}" alt="">
      </div>
      <span>{{displayName this}}{{#if (checkUser this.id)}}(Вы){{/if}}</span>
      </li>
      {{/each}}
    </ul>
  </div>
  <hr style="width: 100%" />
  <div class="form-wrapper">
    <form id="form-add-user">
      {{{ inputUserName }}}
      {{{ formError }}}
      <div class="btns-wrapper-horizontal">
        {{{ buttonCancel }}}
        {{{ buttonCreate }}}
      </div>
    </form>
  </div>
  </div>
</div>
</div>